class HelloWorld {
    public static void main(String[] args) {
        int[] a={10,20,30,40,50};
        int i=0;
        int j=0;
        while(j<a.length) {
            for (int k = 0; k <= 2; k++) {
                System.out.print(a[k] + " ");
            }
            System.out.println();
            i++;
            j++;

        }
    }
}
